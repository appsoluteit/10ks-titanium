<Alloy>
	<!-- iOS -->
	<NavigationWindow id="members" platform="ios">
		<Window title="Members" onOpen="window_open" id="window">
		    <LeftNavButton>
				<View width="Ti.UI.SIZE" height="30">
                	<Button image="/common/chevrons/left-16-w.png" title="Back" onClick="btnBack_click" style="Ti.UI.iOS.SystemButtonStyle.PLAIN"/>
				</View>
            </LeftNavButton>

			<Require id="membersView" src="tournaments/teamMembersView" type="view"/>
		</Window>
	</NavigationWindow>
	
	<!-- Android -->
	<Window id="members" title="10,000 Steps" platform="android" onOpen="window_open">
        <ActionBar 
        	id="actionbar" 
        	title="Members" 
        	homeButtonEnabled="true"
        	displayHomeAsUp="true"
        	onHomeIconItemSelected="btnBack_click" />

		<Require id="membersView" src="tournaments/teammembersView" type="view"/>
    </Window>
</Alloy>