<Alloy>
	<!-- iOS -->
	<Window id="login" title="Login" onOpen="window_open" platform="ios">
		<Require id="loginView" src="auth/loginView" type="view"></Require>	
	</Window>
	
	<!-- Android -->
	<Window id="login" title="login" platform="android" onAndroid:back="androidBack_click" onOpen="window_open">
		<ActionBar title="Login"></ActionBar>
		<Require id="loginView" src="auth/loginView" type="view"></Require>	
	</Window>	
</Alloy>