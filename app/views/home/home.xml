<Alloy>
	<!-- iOS -->
	<NavigationWindow id="home" platform="ios">
		<Window title="Home" onOpen="window_open">
			<Require id="homeView" src="home/homeView" type="view"/>
		</Window>
	</NavigationWindow>
	
	<!-- Android -->
	<Window id="home" platform="android" onAndroid:back="androidBack_click" onOpen="window_open">
		<ActionBar 
			title="Home" 
			homeButtonEnabled="false"></ActionBar>
			
		<Require id="homeView" src="home/homeView" type="view"/>
	</Window>
</Alloy>